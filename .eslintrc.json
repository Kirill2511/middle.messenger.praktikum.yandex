{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:import/errors",
    "plugin:import/warnings",
    "airbnb-base",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  }
}
